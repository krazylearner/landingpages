	function ExpandCollapse()
	{
		if (document.getElementById('footnotes').style.display == 'block')
		{
			document.getElementById('footnotes').style.display = 'none';
			document.getElementById('expand-collapse').innerHTML = '+';
		}
		else
		{
			document.getElementById('footnotes').style.display = 'block';
			document.getElementById('expand-collapse').innerHTML = '-';
		}
	}
</script>

